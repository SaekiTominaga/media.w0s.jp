		"@w0s/sqlite-utility": "2.0.0",
		"better-sqlite3": "12.6.2",
		"hono": "4.11.7",
		"image-size": "2.0.2",
		"kysely": "0.28.9",
		"log4js": "6.9.1",
		"qs": "6.14.1",
		"sharp": "0.34.5",
		"whatwg-mimetype": "4.0.0"
	},
	"devDependencies": {
		"@types/better-sqlite3": "7.6.13",
		"@types/node": "24.10.0",
		"@types/qs": "6.14.0",
		"@types/whatwg-mimetype": "3.0.2",
		"@w0s/eslint-config": "9.4.1",
		"@w0s/stylelint-config": "4.17.0",
		"@w0s/tsconfig": "1.7.0",
		"prettier": "3.8.1",
		"rimraf": "6.1.2",
		"tsx": "4.21.0",
		"typescript": "5.9.3"
	},
	"packageManager": "pnpm@10.28.1"
}
