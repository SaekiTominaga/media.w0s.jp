{
	"name": "media.w0s.jp",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "tsx --watch --tsconfig node/tsconfig.json node/src/app.ts",
		"prebuild": "rimraf node/dist/* -g",
		"build": "tsc -p node/tsconfig.json",
		"lint": "pnpm run /^lint:.*/",
		"lint:js": "eslint node/@types/*.d.ts node/src/**/*.ts",
		"lint:css": "stylelint public/*.css",
		"test": "node --experimental-test-coverage --env-file-if-exists=.env.test --test node/src/**/*.test.ts"
	},
	"dependencies": {
		"@hono/node-server": "1.19.9",
		"@log4js-node/smtp": "2.0.8",
